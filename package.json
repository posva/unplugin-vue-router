{
  "name": "unplugin-vue-router",
  "version": "0.9.0",
  "type": "module",
  "packageManager": "pnpm@9.1.4",
  "description": "File based typed routing for Vue Router",
  "keywords": [
    "vue-router",
    "pages",
    "filesystem",
    "types",
    "typed",
    "router",
    "unplugin",
    "vite",
    "webpack",
    "rollup"
  ],
  "homepage": "https://github.com/posva/unplugin-vue-router#readme",
  "bugs": {
    "url": "https://github.com/posva/unplugin-vue-router/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/unplugin-vue-router.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.ts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": {
        "require": "./dist/vite.d.cts",
        "import": "./dist/vite.d.ts"
      },
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": {
        "require": "./dist/webpack.d.cts",
        "import": "./dist/webpack.d.ts"
      },
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "types": {
        "require": "./dist/rollup.d.cts",
        "import": "./dist/rollup.d.ts"
      },
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": {
        "require": "./dist/esbuild.d.cts",
        "import": "./dist/esbuild.d.ts"
      },
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./options": {
      "types": {
        "require": "./dist/options.d.cts",
        "import": "./dist/options.d.ts"
      },
      "require": "./dist/options.cjs",
      "import": "./dist/options.js"
    },
    "./runtime": {
      "types": {
        "require": "./dist/runtime.d.cts",
        "import": "./dist/runtime.d.ts"
      },
      "require": "./dist/runtime.cjs",
      "import": "./dist/runtime.js"
    },
    "./types": {
      "types": {
        "require": "./dist/types.d.cts",
        "import": "./dist/types.d.ts"
      },
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./data-loaders/basic": {
      "types": {
        "require": "./dist/data-loaders/basic.d.cts",
        "import": "./dist/data-loaders/basic.d.ts"
      },
      "require": "./dist/data-loaders/basic.cjs",
      "import": "./dist/data-loaders/basic.js"
    },
    "./data-loaders/pinia-colada": {
      "types": {
        "require": "./dist/data-loaders/pinia-colada.d.cts",
        "import": "./dist/data-loaders/pinia-colada.d.ts"
      },
      "require": "./dist/data-loaders/pinia-colada.cjs",
      "import": "./dist/data-loaders/pinia-colada.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "./client": [
        "./client.d.ts"
      ],
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "client.d.ts",
    "route.schema.json"
  ],
  "scripts": {
    "build": "pnpm run build:core && pnpm run build:runtime",
    "build:core": "tsup",
    "build:runtime": "tsup --config tsup-runtime.config.ts",
    "dev": "pnpm run test",
    "docs": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "lint": "prettier -c '{src,examples,playground}/**/*.{ts,vue}'",
    "play": "npm -C playground run dev",
    "play:build": "npm -C playground run build",
    "release": "node scripts/release.mjs",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "test": "vitest --typecheck --ui"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.mjs"
  },
  "lint-staged": {
    "*.{js,mjs,json,cjs,md}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/types": "^7.24.6",
    "@rollup/pluginutils": "^5.1.0",
    "@vue-macros/common": "^1.10.4",
    "ast-walker-scope": "^0.6.1",
    "chokidar": "^3.6.0",
    "fast-glob": "^3.3.2",
    "json5": "^2.2.3",
    "local-pkg": "^0.5.0",
    "mlly": "^1.7.0",
    "pathe": "^1.1.2",
    "scule": "^1.3.0",
    "unplugin": "^1.10.1",
    "yaml": "^2.4.3"
  },
  "peerDependencies": {
    "vue-router": "^4.3.0"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  },
  "devDependencies": {
    "@pinia/colada": "^0.6.0",
    "@shikijs/vitepress-twoslash": "1.6.2",
    "@tanstack/vue-query": "^5.40.0",
    "@types/node": "^20.14.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "@volar/vue-language-core": "^1.6.5",
    "@vue/test-utils": "^2.4.6",
    "chalk": "^5.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "enquirer": "^2.4.1",
    "execa": "^9.1.0",
    "firebase": "^10.12.2",
    "happy-dom": "^14.12.0",
    "lint-staged": "^15.2.5",
    "minimist": "^1.2.8",
    "nodemon": "^3.1.2",
    "p-series": "^3.0.0",
    "pinia": "^2.1.7",
    "prettier": "^3.3.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "semver": "^7.6.2",
    "ts-expect": "^1.3.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-markdown": "^0.26.2",
    "unplugin-vue-router": "workspace:*",
    "vite": "^5.2.12",
    "vitepress": "1.2.2",
    "vitest": "^1.6.0",
    "vue": "^3.4.27",
    "vue-router": "4.3.2",
    "vue-router-mock": "^1.1.0",
    "vue-tsc": "^2.0.19",
    "vuefire": "^3.1.23",
    "webpack": "^5.91.0",
    "yorkie": "^2.0.0"
  }
}
